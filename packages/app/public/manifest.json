{
  "name": "limesandbox",
  "version": "0.0.6",
  "manifest_version": 2,
  "description": "Codesandbox on chrome extension",
  "browser_action": {
    "default_title": "Limesandbox"
  },
  "icons": {
    "128": "cocktail.png"
  },
  "background": {
    "scripts": ["background.js"]
  },
  "permissions": [ "tabs", "storage", "http://*/*", "https://*/*" ],
  "content_security_policy": "script-src 'self' 'unsafe-eval' 'sha256-ju0AYilx7SYhKdgDGf8FZNUrIQU+1W7GWgfeTVnZ4KE=' 'sha256-ju0AYilx7SYhKdgDGf8FZNUrIQU+1W7GWgfeTVnZ4KE=' https://cdn.jsdelivr.net https://apis.google.com; object-src 'self';"
}
