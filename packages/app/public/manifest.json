{
  "name": "limesandbox",
  "version": "1.0",
  "manifest_version": 2,
  "browser_action": {
    "default_title": "Limesandbox"
  },
  "icons": {
    "128": "cocktail.png"
  },
  "background": {
    "scripts": ["background.js"]
  },
  "permissions": [ "tabs", "storage", "unlimitedStorage", "http://*/*", "https://*/*" ]
}
